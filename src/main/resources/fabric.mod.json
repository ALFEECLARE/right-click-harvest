{
  "schemaVersion": 1,
  "id": "rightclickharvest",
  "version": "${version}",
  "name": "Right Click Harvest",
  "description": "Allows you to harvest crops with a single right click",
  "authors": [
    "Jamalam"
  ],
  "contributors": [
    "Fourmisain",
    "Jomlom"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/rightclickharvest-fabric",
    "sources": "https://github.com/JamCoreModding/RightClickHarvest",
    "issues": "https://github.com/JamCoreModding/RightClickHarvest/issues"
  },
  "license": "MIT",
  "icon": "assets/rightclickharvest/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.jamalam360.rightclickharvest.RightClickHarvestModInit"
    ],
    "modmenu": [
      "io.github.jamalam360.rightclickharvest.config.ModMenuScreen"
    ],
    "fabric-gametest": [
      "io.github.jamalam360.rightclickharvest.RightClickHarvestGameTest"
    ]
  },
  "mixins": [
    "rightclickharvest.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "jamlib": ">=0.0.3"
  },
  "suggests": {
    "modmenu": "*"
  },
  "custom": {
    "notify_gradle_properties_url": "https://raw.githubusercontent.com/JamCoreModding/RightClickHarvest/1.18.2/gradle.properties",
    "notify_gradle_properties_key": "mod_version"
  }
}
