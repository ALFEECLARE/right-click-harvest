{
  "schemaVersion": 1,
  "id": "rightclickharvest",
  "version": "${version}",
  "name": "Right Click Harvest",
  "description": "Allows you to harvest crops with a right click",
  "authors": [
    "Jamalam"
  ],
  "contributors": [
    "Fourmisain"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/rightclickharvest-fabric",
    "issues": "https://github.com/JamCoreModding/RightClickHarvestFabric/issues",
    "sources": "https://github.com/JamCoreModding/RightClickHarvestFabric"
  },
  "license": "MIT",
  "icon": "assets/rightclickharvest/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.jamalam360.rightclickharvest.RightClickHarvestModInit"
    ],
    "modmenu": [
      "io.github.jamalam360.rightclickharvest.ModMenuScreen"
    ],
    "fabric-gametest": [
      "io.github.jamalam360.rightclickharvest.RightClickHarvestGameTest"
    ]
  },
  "mixins": [
    "rightclickharvest.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.2",
    "fabric": "*"
  },
  "custom": {
    "notify_gradle_properties_url": "https://raw.githubusercontent.com/JamCoreModding/RightClickHarvestFabric/1.16-1.18/gradle.properties",
    "notify_gradle_properties_key": "mod_version"
  }
}
